Algoritmo "ex_011"
var

   a, b, c, delta: real

inicio


   escreva("Digite o primeiro valor A: ")
   leia(a)

   escreva("Digite o segundo valor B: ")
   leia(b)

   escreva("Digite o terceiro valor C: ")
   leia(c)

   delta <- (b^2) - (4 * a * c)

   escreva("Valor de delta é: ", delta)



fimalgoritmo