Algoritmo "ex_42"

//Digite um valor: 35
//Contagem: 1 2 3 4 5 6 7 ... 33 34 35 Acabou!

var

   i, valor: inteiro

inicio

   i <- 1

   escreva("Digite um valor: ")
   leia(valor)

   enquanto ( i <= valor) faca

      escreva(i,"")
      i <- i + 1

   fimenquanto

   escreva(" Acabou!")

fimalgoritmo