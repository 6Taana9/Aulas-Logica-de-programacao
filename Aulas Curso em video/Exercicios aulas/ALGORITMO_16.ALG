algoritmo "algoritmo_16"

// impar ou par

var

   n: inteiro

inicio

      escreva("Digite um numero: ")
      leia(n)
      
      se (n % 2 = 0) entao
         escreva("Esse numero é par")
         
      senao
           escreva("Esse numero é impar")
           
      fimse
      

fimalgoritmo