algoritmo "algoritmo_28"

//Estrutura de repetição

var

   i, fim, salto: inteiro

inicio

   i <- 0
   escreva("Digite até onde vc quer contar: ")
   leia(fim)
   
   escreva("De quanto em quanto voce quer contar: ")
   leia(salto)

   enquanto (i <= fim) faca

      escreval(i)
      i <- i + salto

   fimenquanto
   
   escreva("Fim")

fimalgoritmo